---
platform: linux

image_resource:
  type: docker-image
  source:
    repository: mumoshu/dcind

inputs:
  - name: git-repo
  - name: gradle-project
outputs:
  - name: out

run:
  path: sh
  args:
    - -exc
    - |
      source /docker-lib.sh
      cd git-repo
      start_docker
      docker-compose up
      docker ps
      docker-compose version
